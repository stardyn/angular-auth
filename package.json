{
  "name": "@stardyn/angular-auth",
  "version": "2.0.10",
  "description": "Angular Authentication Service - Lightweight, configurable authentication service for Angular applications with token management and role-based access control",
  "main": "core.js",
  "module": "core.js",
  "exports": {
    ".": {
      "import": "./core.js",
      "types": "./main.d.ts"
    }
  },
  "types": "main.d.ts",
  "sideEffects": false,
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "ng build angular-auth",
    "build:prod": "ng build angular-auth --configuration production",
    "build:npmjs": "pwsh -File ./build_publish_npmjs.ps1",
    "build:private": "pwsh -File ./build_publish.ps1",
    "publish:npmjs": "pwsh -File ./build_publish_npmjs.ps1 -Environment live",
    "publish:private": "pwsh -File ./build_publish.ps1 -Environment live",
    "publish:dry": "pwsh -File ./build_publish.ps1 -DryRun",
    "publish:skip": "pwsh -File ./build_publish.ps1 -SkipBuild",
    "clean": "rimraf dist",
    "dev": "ng build angular-auth --configuration development",
    "test": "echo 'No tests specified'",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major"
  },
  "devDependencies": {
    "@angular/cli": "^20.0.6",
    "@angular-devkit/build-angular": "^20.0.6",
    "@types/node": "^22.0.0",
    "typescript": "~5.8.0",
    "ng-packagr": "^20.0.0",
    "rimraf": "^6.0.0"
  },
  "dependencies": {},
  "peerDependencies": {
    "@angular/common": "^20.0.0",
    "@angular/core": "^20.0.0",
    "@angular/router": "^20.0.0",
    "rxjs": ">=7.0.0",
    "@stardyn/angular-console": "^2.0.9",
    "@stardyn/angular-data-source": "^2.0.9",
    "@stardyn/angular-helpers": "^2.0.9"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "angular",
    "authentication",
    "auth",
    "token-management",
    "role-based-access",
    "permission-guard",
    "typescript",
    "stardyn",
    "angular-auth",
    "oauth",
    "jwt"
  ],
  "author": "Stardyn Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/stardyn/angular-auth.git"
  },
  "homepage": "https://github.com/stardyn/angular-auth",
  "bugs": {
    "url": "https://github.com/stardyn/angular-auth/issues"
  }
}